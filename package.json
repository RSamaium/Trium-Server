  "scripts": {
    "test": "NODE_ENV=test mocha --timeout 0 server/api/**/*.spec.js",
    "test:services": "NODE_ENV=test mocha --timeout 0 server/tests/*.spec.js",
    "apidoc": "apidoc -i server/api/ -o doc/ -f \".*\\.js$\"",
    "start": "supervisor -i ./server/uploads,public,dist ./server/bin/www.js",
    "dev": "cd client && node ./build/dev-server.js",
    "production": "NODE_ENV=production node ./server/bin/www.js",
    "unit": "cd client && cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "cd client && node test/e2e/runner.js",
    "test:client": "npm run unit && npm run e2e",
    "lint": "cd client &&  eslint --ext .js,.vue src client/test/unit/specs test/e2e/specs",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cd client && cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cd client && cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
